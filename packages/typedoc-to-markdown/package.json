{
  "name": "typedoc-to-markdown",
  "version": "0.0.0",
  "description": "A plugin for TypeDoc that enables TypeScript API documentation to be generated in Markdown.",
  "bin": {
    "typedoc-to-markdown": "./bin/cli.js"
  },
  "main": "dist/index.js",
  "files": [
    "dist/"
  ],
  "scripts": {
    "lint": "eslint ./src --ext .ts",
    "prepublishOnly": "yarn run lint && yarn run build && yarn run test",
    "build": "rm -rf dist && tsc",
    "test": "jest --colors --updateSnapshot",
    "build-and-test": "yarn run build && yarn run test",
    "demos": "rm -rf ./demo && npm run build && yarn run demo:md && yarn run demo:html",
    "demo:md": "rm -rf ./demo/md && yarn run build && typedoc-to-markdown --options ./demo.options.json --out ./demo/md",
    "demo:html": "typedoc --options ./demo.options.json --plugin none --out ./demo/html"
  },
  "author": "Thomas Grey",
  "license": "MIT",
  "engines": {
    "node": ">=10.0.0"
  },
  "bugs": {
    "url": "https://github.com/tgreyuk/typedoc-plugin-markdown/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tgreyuk/typedoc-plugin-markdown.git",
    "directory": "packages/typedoc-plugin-markdown"
  },
  "homepage": "https://github.com/tgreyuk/typedoc-plugin-markdown/tree/master/packages/typedoc-plugin-markdown",
  "keywords": [
    "markdown",
    "typescript",
    "documentation",
    "api"
  ],
  "peerDependencies": {
    "typedoc": ">=0.20.0"
  },
  "devDependencies": {
    "typedoc": "0.20.0-beta.27",
    "typescript": "^4.0.5"
  }
}
